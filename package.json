{
  "name": "seline",
  "version": "0.2.3",
  "description": "Seline - Your agents, your rules",
  "author": "Seline",
  "private": true,
  "main": "electron-dist/main.js",
  "overrides": {
    "ai": "^6.0.7",
    "keyv": "5.6.0",
    "tar": ">=7.5.9",
    "minimatch": ">=10.2.2",
    "undici": ">=6.23.0",
    "@electron/asar": ">=4.0.1",
    "glob": ">=11.0.1",
    "@anthropic-ai/claude-agent-sdk": {
      "zod": "$zod"
    }
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "web:dev": "npm rebuild better-sqlite3 && next dev",
    "build": "node scripts/next-build-clean-room.js",
    "start": "next start",
    "typecheck": "tsx scripts/typecheck.ts",
    "typecheck:all": "tsx scripts/typecheck.ts",
    "typecheck:app": "tsc -p tsconfig.app.json --noEmit --pretty false",
    "typecheck:lib": "tsc -p tsconfig.lib.json --noEmit --pretty false",
    "typecheck:electron": "tsc -p tsconfig.electron.json --noEmit --pretty false",
    "typecheck:tooling": "tsc -p tsconfig.tooling.json --noEmit --pretty false",
    "typecheck:ci": "tsx scripts/typecheck.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:validate:migration": "tsx scripts/validation/exec-validation.ts validate-message-ordering-migration",
    "test:validate:migration:dry": "tsx scripts/validation/exec-validation.ts validate-message-ordering-migration --dry-run",
    "test:validate:chat-overflow": "tsx scripts/validation/exec-validation.ts validate-chat-overflow-guards",
    "test:validate:chat-overflow:dry": "tsx scripts/validation/exec-validation.ts validate-chat-overflow-guards --dry-run",
    "test:validate:tool-errors": "tsx scripts/validation/exec-validation.ts validate-tool-error-auditability",
    "test:validate:tool-errors:dry": "tsx scripts/validation/exec-validation.ts validate-tool-error-auditability --dry-run",
    "test:validate:mac-signing": "tsx scripts/validation/exec-validation.ts validate-macos-signing-notarization",
    "test:validate:mac-signing:dry": "tsx scripts/validation/exec-validation.ts validate-macos-signing-notarization --dry-run",
    "test:validate:utility-session-token-audit": "tsx scripts/validation/exec-validation.ts validate-utility-session-token-audit",
    "test:validate:utility-session-token-audit:dry": "tsx scripts/validation/exec-validation.ts validate-utility-session-token-audit --dry-run",
    "test:ddg:windows": "tsx scripts/diagnose-ddg-windows.ts",
    "test:ddg:runtime": "tsx scripts/verify-ddg-runtime.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "electron:compile": "tsc -p tsconfig.electron.json",
    "electron:bundle": "node esbuild.electron.mjs",
    "electron:bundle:dev": "node esbuild.electron.mjs --dev",
    "electron:dev": "npm run typecheck && npm rebuild better-sqlite3 && npm run electron:bundle:dev && concurrently -k \"node scripts/dev-with-stdio-fix.mjs\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "electron:bundle-whisper": "node scripts/bundle-whisper.js",
    "electron:build": "npm run build && npm run electron:rebuild-native && npm run electron:bundle-whisper && npm run electron:prepare && npm run electron:bundle",
    "electron:prepare": "node scripts/electron-prepare.js",
    "electron:verify-package": "node scripts/verify-package.js",
    "electron:copy-remotion": "rm -rf .next/standalone/node_modules/@remotion && cp -r node_modules/@remotion .next/standalone/node_modules/",
    "electron:rebuild-native": "electron-rebuild -f -o better-sqlite3,onnxruntime-node -v 39.2.4",
    "electron:rebuild-standalone": "electron-rebuild -f -m .next/standalone -o better-sqlite3,onnxruntime-node -v 39.2.4",
    "electron:pack": "npm run electron:build && electron-builder --dir && npm run electron:verify-package",
    "electron:dist": "npm run electron:build && electron-builder && npm run electron:verify-package",
    "electron:dist:mac": "npm run typecheck && npm run electron:build && electron-builder --mac && npm run electron:verify-package",
    "electron:dist:win": "npm run typecheck && rimraf dist-electron && npm run electron:build && electron-builder --win --x64 && npm run electron:verify-package",
    "electron:dist:win64": "npm run typecheck && rimraf dist-electron && npm run electron:build && electron-builder --win --x64 && npm run electron:verify-package",
    "electron:dist:win32": "npm run typecheck && rimraf dist-electron && npm run electron:build && electron-builder --win --ia32 && npm run electron:verify-package",
    "embedding:test": "node scripts/embedding-test.mjs",
    "embedding:pipeline-test": "tsx scripts/embedding-pipeline-test.ts",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.2",
    "@ai-sdk/google": "^3.0.2",
    "@ai-sdk/openai": "^3.0.2",
    "@ai-sdk/openai-compatible": "^2.0.2",
    "@anthropic-ai/claude-agent-sdk": "^0.2.51",
    "@assistant-ui/react": "0.11.53",
    "@assistant-ui/react-ai-sdk": "1.1.20",
    "@assistant-ui/react-markdown": "^0.11.5",
    "@hapi/boom": "^10.0.1",
    "@huggingface/hub": "^2.7.1",
    "@huggingface/transformers": "3.0.0",
    "@lancedb/lancedb": "^0.22.3",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@phosphor-icons/react": "^2.1.10",
    "@qdrant/js-client-rest": "^1.16.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@remotion/bundler": "^4.0.382",
    "@remotion/cli": "^4.0.382",
    "@remotion/renderer": "^4.0.382",
    "@slack/bolt": "^4.1.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cron": "^2.0.1",
    "@vscode/ripgrep": "^1.15.9",
    "@whiskeysockets/baileys": "^6.7.12",
    "ai": "^6.0.7",
    "animejs": "^4.2.2",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "canvas-confetti": "^1.9.4",
    "chokidar": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cron": "^4.4.0",
    "discord.js": "^14.25.1",
    "drizzle-orm": "^0.44.7",
    "ffmpeg-static": "^5.3.0",
    "fft.js": "^4.0.3",
    "framer-motion": "^12.23.24",
    "gpt-tokenizer": "^3.4.0",
    "grammy": "^1.35.1",
    "gray-matter": "^4.0.3",
    "jimp": "^1.6.0",
    "jsdom": "^26.0.0",
    "jszip": "^3.10.1",
    "lru-cache": "^11.2.4",
    "lucide-react": "^0.468.0",
    "luxon": "^3.7.2",
    "mathjs": "^15.1.0",
    "meilisearch": "^0.54.0",
    "mime-types": "^3.0.2",
    "nanoid": "^5.0.9",
    "next": "^16.1.6",
    "next-intl": "^4.6.1",
    "node-edge-tts": "^1.2.10",
    "onnxruntime-node": "1.19.2",
    "onnxruntime-web": "^1.17.0",
    "pdf-parse": "^2.4.5",
    "pino": "^9.7.0",
    "puppeteer": "^24.34.0",
    "qrcode": "^1.5.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-shiki": "^0.9.0",
    "remark-gfm": "^4.0.1",
    "remotion": "^4.0.382",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vosk": "^0.3.45",
    "winston": "^3.17.0",
    "zlib-sync": "^0.1.10",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.2",
    "@types/animejs": "^3.1.13",
    "@types/better-sqlite3": "^7.6.13",
    "@types/canvas-confetti": "^1.9.0",
    "@types/lru-cache": "^7.10.9",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.10.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "baseline-browser-mapping": "^2.9.19",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.9",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.25.12",
    "eslint": "^9.16.0",
    "eslint-config-next": "16.1.0",
    "npm": "^11.10.1",
    "postcss": "^8.4.49",
    "rimraf": "^6.1.2",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.17",
    "wait-on": "^9.0.3"
  }
}
