{
  "name": "seline",
  "version": "0.1.0",
  "description": "Seline - Agentic styling assistant",
  "author": "Seline",
  "private": true,
  "main": "electron-dist/main.js",
  "overrides": {
    "ai": "^6.0.7",
    "keyv": "5.6.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "web:dev": "npm rebuild better-sqlite3 && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "electron:compile": "tsc -p tsconfig.electron.json",
    "electron:dev": "npm rebuild better-sqlite3 && concurrently -k \"npm run dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "npm run build && npm run electron:rebuild-native && npm run electron:prepare && npm run electron:compile",
    "electron:prepare": "node scripts/electron-prepare.js",
    "electron:copy-remotion": "rm -rf .next/standalone/node_modules/@remotion && cp -r node_modules/@remotion .next/standalone/node_modules/",
    "electron:rebuild-native": "electron-rebuild -f -o better-sqlite3,onnxruntime-node -v 39.2.4",
    "electron:rebuild-standalone": "electron-rebuild -f -m .next/standalone -o better-sqlite3,onnxruntime-node -v 39.2.4",
    "electron:pack": "npm run electron:build && electron-builder --dir",
    "electron:dist": "npm run electron:build && electron-builder",
    "electron:dist:mac": "npm run electron:build && electron-builder --mac",
    "electron:dist:win": "rimraf dist-electron && npm run electron:build && electron-builder --win",
    "embedding:test": "node scripts/embedding-test.mjs",
    "embedding:pipeline-test": "tsx scripts/embedding-pipeline-test.ts",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.2",
    "@ai-sdk/google": "^3.0.2",
    "@ai-sdk/openai": "^3.0.2",
    "@ai-sdk/openai-compatible": "^2.0.2",
    "@assistant-ui/react": "^0.11.53",
    "@assistant-ui/react-ai-sdk": "^1.1.20",
    "@assistant-ui/react-markdown": "^0.11.5",
    "@hapi/boom": "^10.0.1",
    "@huggingface/hub": "^2.7.1",
    "@lancedb/lancedb": "^0.22.3",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@qdrant/js-client-rest": "^1.16.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@remotion/bundler": "^4.0.382",
    "@remotion/cli": "^4.0.382",
    "@remotion/renderer": "^4.0.382",
    "@slack/bolt": "^4.1.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cron": "^2.0.1",
    "@vscode/ripgrep": "^1.15.9",
    "@whiskeysockets/baileys": "^6.7.12",
    "@xenova/transformers": "^2.17.2",
    "ai": "^6.0.7",
    "animejs": "^4.2.2",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "canvas-confetti": "^1.9.4",
    "chokidar": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cron": "^4.4.0",
    "drizzle-orm": "^0.44.7",
    "fft.js": "^4.0.3",
    "framer-motion": "^12.23.24",
    "gpt-tokenizer": "^3.4.0",
    "grammy": "^1.35.1",
    "jimp": "^1.6.0",
    "lru-cache": "^11.2.4",
    "lucide-react": "^0.468.0",
    "meilisearch": "^0.54.0",
    "nanoid": "^5.0.9",
    "next": "^15.5.0",
    "next-intl": "^4.6.1",
    "onnxruntime-node": "^1.23.2",
    "onnxruntime-web": "^1.17.0",
    "pdf-parse": "^2.4.5",
    "pino": "^9.7.0",
    "puppeteer": "^24.34.0",
    "qrcode": "^1.5.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-shiki": "^0.9.0",
    "remark-gfm": "^4.0.1",
    "remotion": "^4.0.382",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.2",
    "@types/animejs": "^3.1.13",
    "@types/better-sqlite3": "^7.6.13",
    "@types/canvas-confetti": "^1.9.0",
    "@types/lru-cache": "^7.10.9",
    "@types/node": "^22.10.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.30.4",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.5.0",
    "postcss": "^8.4.49",
    "rimraf": "^6.1.2",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.17",
    "wait-on": "^9.0.3"
  }
}
